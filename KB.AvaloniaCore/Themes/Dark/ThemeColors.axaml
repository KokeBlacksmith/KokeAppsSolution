<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Other -->
    <Color x:Key="LightGreenColor">#59C225</Color>
    <Color x:Key="DarkGreenColor">#57A64A</Color>
    <Color x:Key="DarkRedColor">#8B0000 </Color>

    <!-- Backgrounds -->
    <Color x:Key="DarkestBackgroundColor">#1E1E1E</Color>
    <Color x:Key="DarkBackgroundColor">#252526</Color>


    <!-- Interaction -->
    <Color x:Key="DarkHighLightBackgroundColor">#569CD6</Color>
    <Color x:Key="DarkSelectedBackgroundColor">#00B3AD</Color>
    <Color x:Key="DarkPressedBackgroundColor">#006663</Color>

    <!-- Interaction Movement -->
    <Color x:Key="DarkMovementControlBackgroundColor">LightSlateGray</Color>
    <Color x:Key="DarkMovementControlHighLightBackgroundColor">LightGray</Color>


    <!-- Borders -->
    <Color x:Key="DarkBorderColor">#606060</Color>
    <Color x:Key="DarkBorderSelectionHighLightColor">#455559</Color>


    <!-- Text -->
    <Color x:Key="DarkTextColor">#D4D4D4</Color>


</ResourceDictionary>
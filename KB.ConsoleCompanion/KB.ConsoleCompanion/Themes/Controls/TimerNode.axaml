<ResourceDictionary xmlns="https://github.com/avaloniaui"
					xmlns:kbGraph="clr-namespace:KB.AvaloniaCore.Controls.GraphEditor;assembly=KB.AvaloniaCore"
					xmlns:kbControls="clr-namespace:KB.AvaloniaCore.Controls;assembly=KB.AvaloniaCore"
					xmlns:kbLocalNodes="clr-namespace:KB.ConsoleCompanion.Nodes;assembly=KB.ConsoleCompanion"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">



	<Design.PreviewWith>
		<Border Width="600" Height="600">
			<kbGraph:GraphCanvas>
				<kbGraph:GraphCanvas.ChildNodes>
					<kbLocalNodes:TimerNode />
				</kbGraph:GraphCanvas.ChildNodes>
			</kbGraph:GraphCanvas>
		</Border>
	</Design.PreviewWith>

	<ControlTheme x:Key="{x:Type kbLocalNodes:TimerNode}" TargetType="kbLocalNodes:TimerNode">
		<Setter Property="ClipToBounds" Value="False" />
		<Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="VerticalAlignment" Value="Stretch" />

		<Setter Property="Template">
			<ControlTemplate>
				<Border Width="300" Height="300" Background="Aqua">
					
				</Border>
			</ControlTemplate>
		</Setter>
	</ControlTheme>


</ResourceDictionary>
